{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alfonsobries/nova-froala-editor": "^1.0",
        "bolechen/nova-activitylog": "^0.2.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/nova-file-upload-field": "^0.4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.2.4",
        "infinety-es/nova-filemanager": "^2.8",
        "intervention/image": "^2.5",
        "jorenvanhocht/laravel-share": "^3.3",
        "joshua060198/nova-editable-status-card": "^0.0.8",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^1.6",
        "laravel/nova": "^3.15.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "reinink/remember-query-strings": "^0.1.0",
        "spatie/laravel-permission": "^3.18",
        "tightenco/ziggy": "^0.9.4",
        "vyuldashev/nova-permission": "^2.10"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install": [
            "@php artisan nova:install",
            "@php artisan jetstream:install livewire",
            "@php artisan jetstream:install inertia",
            "@php artisan migrate",
            "@php artisan vendor:publish --all"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
